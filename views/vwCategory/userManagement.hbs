</br>
<h4 class="text-center">
  Users
</h4>
<div class="card shadow mb-4"></div>
{{#if this.empty}}
<div class="card-body">
  <p class="card-text">
    Không có dữ liệu.
  </p>
</div>
{{else}}
<table class="table table-hover">
  <thead>
    <tr>
      <th>Sản phẩm</th>
      <th class="text-center">#</th>
      <th class="text-center">Họ và tên</th>
      <th class="text-center">Email</th>
      <th class="text-center">Loại người dùng</th>
      <th class="text-center">Điểm cộng</th>
      <th class="text-center">Điểm trừ</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    {{#each this.user}}
    {{#if isReal}}
    <tr>
      <th scope="row" class="text-center">{{id}}</th>
      <td>{{full_name}}</td>
      <td>{{email}}</td>
      <td>{{address}}</td>
      <td>{{type_of_user}}</td>
      <td>{{add_point}}</td>
      <td>{{minus_point}}</td>
    </tr>
    {{/if}}
    {{/each}}
  </tbody>
</table>
{{/if}}
</div>